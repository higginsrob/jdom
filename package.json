{
    "name": "jdom",
    "version": "3.2.5",
    "description": "lightweight dom builder",
    "main": "index.js",
    "scripts": {
        "predocs": "rm -rf docs",
        "preversion": "npm run prettier",
        "prettier": "prettier --loglevel=error --write '**/*.js' '**/*.json' '**/*.jsx' '**/*.css' '**/*.md'",
        "start": "node --openssl-legacy-provider ./node_modules/.bin/webpack-dev-server",
        "prebuild": "rm -rf ./dist",
        "build": "node --openssl-legacy-provider ./node_modules/.bin/webpack-cli --env NODE_ENV=production",
        "pretest": "npm run build",
        "posttest": "rm -rf ./dist",
        "test": "node --openssl-legacy-provider ./node_modules/.bin/mocha test"
    },
    "author": "Rob Higgins <higginsrob@gmail.com>",
    "license": "MIT",
    "keywords": [
        "dom",
        "builder",
        "javascript",
        "functional",
        "svg",
        "elements",
        "factory",
        "lightweight",
        "zero-dependencies",
        "browser"
    ],
    "homepage": "https://higginsrob.github.io/jdom",
    "bugs": {
        "url": "https://github.com/higginsrob/jdom/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/higginsrob/jdom.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "babel-eslint": "^10.0.3",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-loader": "^2.2.1",
        "eslint-plugin-prettier": "^3.1.2",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^3.1.0",
        "lint-staged": "^12.3.3",
        "mocha": "^9.2.0",
        "prettier": "^1.19.1",
        "prettier-eslint": "^13.0.0",
        "puppeteer": "^24.23.0",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.3"
    }
}
